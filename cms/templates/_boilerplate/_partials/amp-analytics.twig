<amp-analytics type="googleanalytics" id="analytics2">
    <script type="application/json">
    {
        "vars": {
            "account": "{{ gaTrackingId }}"
        },
        "triggers": {
        "trackPageviewWithAmpdocUrl": {
            "on": "visible",
            "request": "pageview",
            "vars": {
                "title": "{{ seomatic.meta.seoTitle }}",
                "ampdocUrl": "{{ siteUrl(craft.app.request.pathInfo) }}"
                }
            }
        }
    }
    </script>
</amp-analytics>
